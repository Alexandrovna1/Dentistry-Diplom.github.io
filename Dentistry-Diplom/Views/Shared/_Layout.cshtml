<!DOCTYPE html>
<html>
<head data-bs-theme="dark">
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="~/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/css/Style.css" rel="stylesheet" />
</head>
<body data-bs-theme="dark">
    <div class="container">
        <nav class="navbar navbar-expand-lg bg-body-tertiary" data-bs-theme="dark">
            <div class="container-fluid">
                <a class="navbar-brand" href="#">PERFECT SMILE</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-theme="dark" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                        <li class="nav-item active">
                            <a class="nav-link" aria-current="page" onclick="location.href = '@Url.Action("Index","Home")'" href="/">Главная</a>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" role="button">Отделения стоматологии</a>
                            <ul class="dropdown-menu">
                                <li><a class="dropdown-item" href="#" onclick="location.href = '@Url.Action("TerapevtCategory", "Category")'">Терапевтическое отделение</a></li>
                                <li><a class="dropdown-item" href="#" onclick="location.href = '@Url.Action("HirurgCategory", "Category")'">Хирургическое отделение</a></li>
                                <li><a class="dropdown-item" href="#" onclick="location.href = '@Url.Action("OrtodontCategory", "Category")'">Ортодонтическое отделение</a></li>
                                <li><a class="dropdown-item" href="#" onclick="location.href = '@Url.Action("OrtopedCategory", "Category")'">Ортопедическое отделение</a></li>
                            </ul>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" role="button">Наши специалисты</a>
                            <ul class="dropdown-menu">
                                <li><a class="dropdown-item" href="#" onclick="location.href = '@Url.Action("SpecialistCategory", "Category")'">Список наших специалистов</a></li>
                            </ul>
                        </li>
                        </li>
                    </ul>
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a class="nav-link" href="#" onclick="location.href='@Url.Action("Index","ShopDent")'">Запись на прием</a>
                        </li>

                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" role="button" data-bs-toggle="dropdown">
                                Дополнительно
                            </a>
                            <ul class="dropdown-menu">
                                <li><a class="dropdown-item" href="#" onclick="location.href = '@Url.Action("DentistryList", "Dentistry")'">Список услуг</a></li>
                                <li><a class="dropdown-item" href="#" onclick="location.href = '@Url.Action("AddAllDentistrys", "Home")'">Перегрузка БД</a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </div>
    <div class="container">
        @RenderBody()
    </div>
    <!-- Footer -->
    <footer class="text-center text-lg-start bg-body-tertiary text-muted">
        <!-- Section: Social media -->
        <section class="d-flex justify-content-center justify-content-lg-between p-4 border-bottom">
            <!-- Left -->
            <div class="me-5 d-none d-lg-block">
            </div>
            <!-- Left -->
            <!-- Right -->
            <div>
                <a href="" class="me-4 text-reset">
                    <i class="fab fa-facebook-f"></i>
                </a>
                <a href="" class="me-4 text-reset">
                    <i class="fab fa-twitter"></i>
                </a>
                <a href="" class="me-4 text-reset">
                    <i class="fab fa-google"></i>
                </a>
                <a href="" class="me-4 text-reset">
                    <i class="fab fa-instagram"></i>
                </a>
                <a href="" class="me-4 text-reset">
                    <i class="fab fa-linkedin"></i>
                </a>
                <a href="" class="me-4 text-reset">
                    <i class="fab fa-github"></i>
                </a>
            </div>
            <!-- Right -->
        </section>
        <!-- Section: Social media -->
        <!-- Section: Links  -->
        <section class="">
            <div class="container text-center text-md-start mt-5">
                <!-- Grid row -->
                <div class="row mt-3">
                    <!-- Grid column -->
                    <div class="col-md-3 col-lg-4 col-xl-3 mx-auto mb-4">
